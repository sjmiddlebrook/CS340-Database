<h1>Current Continents:</h1>
<table class="table">
    <thead> 
        <th>Name</th>
    </thead> 
    <tbody> 
        {{#each continents}}
        <tr> 
            <td>{{name}}</td>
            <td><a href="/continents/{{id}}"><button class="btn btn-secondary">Update</button></a></td>
        </tr> 
        {{/each}} 
    </tbody>
</table>


